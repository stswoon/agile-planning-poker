{
    "name": "backend",
    "version": "0.0.1",
    "license": "ISC",
    "scripts": {
        "dev": "nodemon",
        "prod": "npm run lint && npm run build",
        "start": "node dist/server.js",
        "lint:staged": "npm run lint-staged",
        "-----": "echo 0",
        "lint-staged": "lint-staged",
        "build": "tsc",
        "lint": "npm run lint:eslint && npm run lint:prettier",
        "lint:eslint": "eslint .",
        "lint:prettier": "prettier --check src/.",
        "lint:prettier:fix": "prettier --write src/.",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
        "compression": "^1.8.0",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^5.1.0"
    },
    "devDependencies": {
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.1",
        "@types/node": "^22.14.1",
        "eslint": "^9.24.0",
        "eslint-config-prettier": "^10.1.2",
        "lint-staged": "^15.5.1",
        "nodemon": "^3.1.9",
        "prettier": "^3.5.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.26.1"
    },
    "lint-staged": {
        "**/*.{ts,css,json}": [
            "prettier --write"
        ],
        "**/*.{ts}": [
            "eslint"
        ]
    }
}
